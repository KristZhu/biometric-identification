   <children>
      <Pane layoutX="14.0" layoutY="15.0" prefHeight="449.0" prefWidth="542.0" style="-fx-border-color: #005582;">
         <children>
            <HBox layoutX="34.0" layoutY="14.0" prefHeight="46.0" prefWidth="472.0">
